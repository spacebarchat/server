		"@types/dotenv": "^8.2.0",
		"@types/express": "^4.17.12",
		"@types/i18next-node-fs-backend": "^2.1.0",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/morgan": "^1.9.3",
		"@types/multer": "^1.4.7",
		"@types/node": "^18.0.6",
